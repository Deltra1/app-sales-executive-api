{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "ter-arrow-parens": false,
    "no-console": {
      "severity": "warn",
      "options": ["log"]
    },
    "max-line-length": [true, 150]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
